.page{
  padding: 20px
}

.intro {
  margin: 30px;
  text-align: center;
}

.img{
  width: 200rpx;
  height: 200rpx;
  position: relative;
}

.img image{
  width: 100%;
  height: 100%;
}

.text {
  width: 100%;
  margin-bottom: 80rpx;
  font-size: 14px;
  padding: 10rpx;
}

.moment_img {
  width: 200rpx;
  height: 200rpx;
}

.image_content{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 20px;
  padding-bottom: 30rpx;
  border-bottom: 1rpx solid #F1F1F1;
}

.delete-btn {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 30px;
  font-size: 12px;
  color: #fff;
  background: rgba(0,0,0,.5);
  text-align: center;
  line-height: 30px;
}
.btn-release {
  /* background-color: #23A257; */
  width: 250rpx;
  height: 80rpx;
  color: #fff;
  background: linear-gradient(#FF6F00, #FF9100);
  border-radius: 40rpx;
  margin: 0 auto;
  text-align: center;
  line-height: 80rpx;
}