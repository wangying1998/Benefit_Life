.container{
    background-color: #fff;
}
.meal-img {
    width: 100%;
    height: 300rpx;
    display: block;
}
.list-circle {
    width: 20rpx;
    height: 20rpx;
    border: 2px solid #FFD700;
    box-sizing: border-box;
    border-radius: 50%;
    display: inline-block;
    margin-right: 15rpx;
}
.desc-item {
    border-bottom: #f0f0f0 1px solid;
    padding: 20rpx 0;
}
.item-title {
    color: #299939;
    font-size: 32rpx;
}
.meal{
    padding: 30rpx 40rpx;
    background-color: #fff;
}
.meal-name {
    color: #333333;
    text-align: center;
    margin-bottom: 20rpx;
}
.meal-desc {
    color: #999999;
    text-indent: 2em;
    line-height: 1.6;
    font-size: 28rpx;
}
